<Window x:Class="ItemCreator.ItemImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ItemCreator"
        mc:Ignorable="d"
        Title="ItemImportWindow" Height="371" Width="761.55" Loaded="Window_Loaded">
    <Grid>
        <ListView x:Name="ImportBox" ItemsSource="{Binding ImportableItems, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:ItemImportWindow}}}"/>
    </Grid>
</Window>
